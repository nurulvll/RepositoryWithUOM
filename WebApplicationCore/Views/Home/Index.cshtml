@model Demo.Models.Customer

@{
    ViewData["Title"] = "Index";
    var message = ViewBag.SuccessMsg;
}


<div class="container-fluid">
    <div class="card">

        <div class="card-header p-2">
            <button type="button" title="Add New Requisition"
                    data-url="/Home/Create" onclick="GoTo(this)"
                    id="" class="NewButton btn-lg">
                <i class="fa fa-plus" aria-hidden="true"></i>
                &nbsp;
                New
            </button>
       
        </div>

    </div>
</div>


<div class="container-fluid">

    <div class="card card-secondary">
        <div class="card-header">
            <h3 class="card-title">
                List Of Data
            </h3>
        </div>

        <div class="card-body">
            <div class="table-responsive">

                <table class="table table-bordered table-striped" style="width:100%" id="CustomerList">
                    <thead>
                        <tr>
                            <th class="action"><input type="checkbox" class="chkAll" />&nbsp;Action</th>
                            <th>Name</th>
                            <th>Code</th>
                            <th>Address </th>
                            <th> Country </th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>

            </div>
        </div>


    </div>
</div>

